{
  "name": "nebu",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy-pw": "npx tsx ops/deploy.ts promiseWrapped=true",
    "deploy": "npx tsx ops/deploy.ts promiseWrapped=false",
    "dev": "vite",
    "build:types": "tsc -b",
    "build-dev": "run-p type-check \"build-only {@}\" -- --watch && vite",
    "build-for-index-html": "run-p type-check \"build-only {@}\" -- && cp production.env ./dist/.env",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "build-watch": "vite build --watch",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv-expand": "^12.0.1",
    "naive-ui": "^2.41.0",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-google-login": "^2.0.33"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "1.1.31",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.20.1",
    "eslint-plugin-oxlint": "^0.15.10",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.10",
    "prettier": "^3.5.1",
    "tsx": "^4.19.2",
    "typescript": "~5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.2"
  }
}
